<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="rootParent" prefHeight="76.0" prefWidth="358.0" style="-fx-background-color: #2b2b2b;" styleClass="playbox" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fx:id="fileIcon" fitHeight="61.0" fitWidth="69.0" layoutX="7.0" layoutY="7.0" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@../images/defaul/icons/icons8_albums_24px_2.png" />
         </image>
      </ImageView>
      <Label fx:id="txtTitle" layoutX="84.0" layoutY="22.0" text="Work" textFill="#fffdfd">
         <font>
            <Font name="Calibri" size="16.0" />
         </font>
      </Label>
      <Label fx:id="txtSubtitle" layoutX="85.0" layoutY="38.0" text="Do it again" textFill="#fffdfd">
         <font>
            <Font name="Calibri" size="14.0" />
         </font>
      </Label>
      <Label fx:id="txtTime" layoutX="277.0" layoutY="31.0" text="20:40" textFill="#fffdfd">
         <font>
            <Font name="Calibri" size="12.0" />
         </font>
      </Label>
      <ImageView fitHeight="15.0" fitWidth="15.0" layoutX="336.0" layoutY="31.0" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@../images/defaul/icons/icons8_right_arrow_32px.png" />
         </image>
      </ImageView>
   </children>
   <effect>
      <DropShadow blurType="ONE_PASS_BOX" color="#323232" />
   </effect>
</Pane>
